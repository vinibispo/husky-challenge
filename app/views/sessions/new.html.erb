<%= form_with(model: session_model, data: { turbo: false }) do |f| %>
  <div class="form-group">
    <%= f.label :token, style: 'display: block;', class: 'col-sm-2 control-label' %>
    <%= f.text_field :token, class: 'form-control col-sm-10' %>
  </div>
  <div class="form-group">
    <%= f.submit I18n.t(create_or_update(session_model), scope: 'forms.buttons.sessions'), class: 'btn btn-primary', style: 'margin-top: 10px' %>
  </div>
<% end %>
